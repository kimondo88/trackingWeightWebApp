{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    //"extends": "eslint:recommended",
    "parserOptions": {
        "sourceType" : "module", 
        "ecmaVersion": 12
    },
    "rules": {
        "no-eval": "error",
        "no-implied-eval": "error",
        "no-new-func": "error"
    }
}
